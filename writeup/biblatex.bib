@article{plug_spatial_2011,
	title = {Spatial and temporal visualisation techniques for crash analysis},
	volume = {43},
	issn = {0001-4575},
	url = {http://www.sciencedirect.com/science/article/pii/S0001457511001266},
	doi = {10.1016/j.aap.2011.05.007},
	abstract = {Understanding the underlying structure of single vehicle crashes ({SVCs}) is essential for improving safety on the roads. Past research has found that {SVCs} tend to cluster both spatially and temporally. However, limited research has been conducted to investigate the interaction between the location of {SVCs} and the time they occur, especially at different levels of scales or spatial extents. This paper applied spatial, temporal and spatio-temporal techniques to investigate patterns of {SVCs} in Western Australia between 1999 and 2008, at different levels of scale. Spider graphs were adapted to identify temporal patterns of vehicle crashes at two different levels of scales: daily and weekly with respect to their causes. The spatial structures of vehicle crashes were analysed using Kernel Density Estimation analysis at three different scales: West Australia, Metropolitan area, and Perth Local Government Area ({LGA}). These are illustrated using spatial zooming theory. Comap was then used to demonstrate the spatio-temporal interaction effect on vehicle crashes. The results show significant differences in spatio-temporal patterns of {SVCs} for various crash causes. The techniques used here have the potential to help decision makers in developing effective road safety strategies.},
	pages = {1937--1946},
	number = {6},
	journaltitle = {Accident Analysis \& Prevention},
	shortjournal = {Accident Analysis \& Prevention},
	author = {Plug, Charlotte and Xia, Jianhong (Cecilia) and Caulfield, Craig},
	urldate = {2020-12-13},
	date = {2011-11-01},
	langid = {english},
	keywords = {Comap, Geographic Information Systems ({GIS}), Kernel density estimation, Scale, Single vehicle crashes ({SVCs}), Spatial and temporal visualisation, Spatio-temporal zooming theory, Spider graph},
	file = {ScienceDirect Full Text PDF:/Users/anjadoppelmayr/Zotero/storage/C5R3W3JD/Plug et al. - 2011 - Spatial and temporal visualisation techniques for .pdf:application/pdf;ScienceDirect Snapshot:/Users/anjadoppelmayr/Zotero/storage/J67IT63H/S0001457511001266.html:text/html}
}

